type W=Int64;function p(b:W):W;begin p:=0;while b<>0do begin p+=1;b:=b and(b-1);end;end;var N,M,t,i,j,A,B,x,y:W;D:Array[0..9]of W;_:Char;z:String;begin readln(N,M);for i:=0to N-1do begin read(_);while _<>' 'do read(_);readln(z);t:=0;for j:=0to M-1do if z[j+1]='Y'then t+=W(1)shl j;D[i]:=t end;A:=0;B:=-1;for i:=0to(1<<N)-1do begin t:=0;for j:=0to N-1do if(i and(1<<j))<>0then t:=t or D[j];y:=p(i);x:=p(t);if x>A then begin A:=x;B:=y end else if(x=A)and(y<B)then B:=y end;writeln(B)end.
